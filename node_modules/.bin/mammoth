#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/patrick/dev/projects/mine/ai/ai-sandbox/sanechain/node_modules/.pnpm/mammoth@1.5.1/node_modules/mammoth/bin/node_modules:/home/patrick/dev/projects/mine/ai/ai-sandbox/sanechain/node_modules/.pnpm/mammoth@1.5.1/node_modules/mammoth/node_modules:/home/patrick/dev/projects/mine/ai/ai-sandbox/sanechain/node_modules/.pnpm/mammoth@1.5.1/node_modules:/home/patrick/dev/projects/mine/ai/ai-sandbox/sanechain/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/patrick/dev/projects/mine/ai/ai-sandbox/sanechain/node_modules/.pnpm/mammoth@1.5.1/node_modules/mammoth/bin/node_modules:/home/patrick/dev/projects/mine/ai/ai-sandbox/sanechain/node_modules/.pnpm/mammoth@1.5.1/node_modules/mammoth/node_modules:/home/patrick/dev/projects/mine/ai/ai-sandbox/sanechain/node_modules/.pnpm/mammoth@1.5.1/node_modules:/home/patrick/dev/projects/mine/ai/ai-sandbox/sanechain/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../mammoth/bin/mammoth" "$@"
else
  exec node  "$basedir/../mammoth/bin/mammoth" "$@"
fi
